<a-text value="{{title}}" position="{{x}} 8 {{z}}" width="25" align="center"></a-text>
<a-text value="{{title}}" position="{{x}} 8 {{z}}" width="25" align="center" rotation="0 180 0"></a-text>
<a-box
    id="track-box-{{id}}"
    class="audio-box"
    material="color: {{color}}"
    depth="5"
    height="10"
    width="5"
    position="{{x}} {{y}} {{z}}"
    rotation="0 45 0"
    audio-box="on: click">
</a-box>
